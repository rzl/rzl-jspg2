{
  "name": "rzl-jspg2",
  "displayName": "rzl-jspg2",
  "description": "rzl-jspg2",
  "version": "0.0.7",
  "publisher": "rzl",
  "icon": "R.png",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:rzl-jspg2.helloWorld",
    "onCommand:rzl-jspg2.createConfig",
    "workspaceContains:.vscode/jspg2.json"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "onlineExplorer",
          "name": "onlineExplorer",
          "when": "rzl-jspg2.enabled"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "onlineExplorer.refresh",
          "when": "view == onlineExplorer",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "when": "rzl-jspg2.enabled",
          "command": "rzl-jspg2.loadCode",
          "group": "navigation"
        },
        {
          "when": "rzl-jspg2.enabled",
          "command": "rzl-jspg2.createConfig",
          "group": "navigation"
        },
        {
          "when": "rzl-jspg2.enabled",
          "command": "rzl-jspg2.get",
          "group": "navigation"
        },
        {
          "when": "rzl-jspg2.enabled",
          "command": "rzl-jspg2.put",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "jsonOutline.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "onlineExplorer.openResource",
        "title": "openResource",
        "category": "rzl-jspg2"
      },
      {
        "command": "onlineExplorer.refresh",
        "title": "refresh",
        "category": "rzl-jspg2"
      },
      {
        "command": "rzl-jspg2.get",
        "title": "get",
        "category": "rzl-jspg2"
      },
      {
        "command": "rzl-jspg2.put",
        "title": "put",
        "category": "rzl-jspg2"
      },
      {
        "command": "rzl-jspg2.createConfig",
        "title": "create config",
        "category": "rzl-jspg2"
      },
      {
        "command": "rzl-jspg2.loadCode",
        "title": "load code",
        "category": "rzl-jspg2"
      },
      {
        "command": "rzl-jspg2.helloWorld",
        "title": "Hello World",
        "category": "rzl-jspg2"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "fs-extra": "^11.1.0",
    "jsonc-parser": "^3.2.0",
    "md5": "^2.3.0",
    "vue": "^2.6.14"
  }
}
